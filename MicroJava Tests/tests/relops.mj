program Test
{
  void main()
    int a;
  {
	read(a);
	if (a == 1) { print('='); print('='); print(','); }
	if (a != 1) { print('!'); print('='); print(','); }
	if (a < 1)  { print('<'); print(','); }
	if (a <= 1) { print('<'); print('='); print(','); }
	if (a > 1)  { print('>'); print(','); }
	if (a >= 1) { print('>'); print('='); print(','); }
  }
}